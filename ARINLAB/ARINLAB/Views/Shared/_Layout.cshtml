                    <h2 style="font-size: 20px; font-weight: bold;">https://www.arinlang.com/</h2> <button onclick="Copy()"><img style="margin-right: 15px;" src="~/images/Icons/Copy.svg"></button>
                </div>
                <div class="flex flex-row justify-evenly" style="margin-top: 20px;">
                    <a style="padding: inherit;" href="@(_settingsService.GetAllSettings().FirstOrDefault(p => p.Name.Contains("mail"))?.Value)"><img style="width: 80px; height: auto;" src="~/images/logo/Mail+Folder.png"></a>
                    <a style="padding: inherit;" href="@(_settingsService.GetAllSettings().FirstOrDefault(p => p.Name.Contains("twit"))?.Value)"><img style="width: 80px; height: auto;" src="~/images/logo/301280585154211.png"></a>
                    <a style="padding: inherit;" href="@(_settingsService.GetAllSettings().FirstOrDefault(p => p.Name.Contains("inst"))?.Value)"><img style="width: 80px; height: auto;" src="~/images/logo/b414768e7af1948afcbafb9bd4b0fd12.png"></a>
                    <a style="padding: inherit;" href="@(_settingsService.GetAllSettings().FirstOrDefault(p => p.Name.Contains("whats"))?.Value)"><img style="width: 80px; height: auto;" src="~/images/logo/986-9860348_o-software-est-disponvel-para-android-blackberry-whatsapp.png"></a>
                </div>
            </div>


            <!--///////////////////////////////////  Modal for Dictionary/////////////////////////////////////////////////////////-->
            <div class="container absolute bg-white" id="Dictionary" style="z-index: 100; display: none; margin-top: 10px; top: 8%; left:35%;  width: 490px; height: 350px; border-radius: 15px; border: 1px solid silver;">
                <div style="background-color: #282848; padding: 20px; color: white; border-radius: 15px;">
                    <span style="font-size: 20px; font-weight: bold;">Share</span>
                    <span onclick="OpenDict()" style="margin-left:85%; cursor:pointer;">X</span>
                </div>
                <div class="justify-center align-center" style="width: 350px; height: auto; margin: auto; padding-top: 15px;">
                    <img src="~/images/Logo/61097513bc084caf8d47cc6492401d95.png">
                </div>

                <div class="sm:w-8/12 sm:px-0 px-4 mx-auto w-full">
                    <form asp-area="" asp-controller="Home" asp-action="SetDictionary" method="get">
                        <div class="sm:search-box grid gap-y-8 relative sm:overflow-hidden sm:rounded" style="width:100%">
                            <select name="dictId" style="margin-right: 30%; background: aliceblue;"
                                    class="h-12 w-full sm:w-10/12 outline-none px-4 sm:border-none border border-gray-200 rounded sm:rounded-none"
                                    id="dictionary-input">
                                @foreach (var dictItem in dicts)
                                {
                                    <option value="@dictItem.Id">@dictItem.Language</option>
                                }
                            </select>
                            <div class="sm:absolute right-0 top-0 h-12 z-10 flex items-center sm:w-2/12 box-border">
                                <button type="submit" class="bg-secondary text-white sm:rounded-none rounded w-full px-8 py-3 font-semibold text-center cursor-pointer select-none">
                                    @SharedLocalizer["Select Dictionary"]
                                </button>
                            </div>
                        </div>
                    </form>

                </div>

            </div>

            <!--///////////////////////////////////  Modal for Search/////////////////////////////////////////////////////////-->
            <div class="container absolute bg-white" id="Search" style="z-index: 100; display: none; margin-top: 10px; top: 8%; left:35%;  width: 490px; height: 350px; border-radius: 15px; border: 1px solid silver;">
                <div style="background-color: #282848; padding: 20px; color: white; border-radius: 15px;">
                    <span style="font-size: 20px; font-weight: bold;">Share</span>
                    <span onclick="OpenSearch()" style="margin-left:85%; cursor:pointer;">X</span>
                </div>
                <div class="justify-center align-center" style="width: 350px; height: auto; margin: auto; padding-top: 15px;">
                    <img src="~/images/Logo/61097513bc084caf8d47cc6492401d95.png">
                </div>

                <div class="sm:w-8/12 sm:px-0 px-4 mx-auto w-full">
                    <form asp-area="" asp-controller="Search" asp-action="Search" method="get">
                        <div class="sm:search-box grid gap-y-8 relative sm:overflow-hidden sm:rounded" >
                            <input name="term" type="text" style="margin-right:10%; background-color:aliceblue"
                                   class="h-12 w-full sm:w-10/12 outline-none px-4 sm:border-none border border-gray-200 rounded sm:rounded-none"
                                   id="search-input" required />
                            <div class="sm:absolute right-0 top-0 h-12 z-10 flex items-center sm:w-2/12 box-border">
                                <button type="submit" class="bg-secondary text-white sm:rounded-none rounded w-full px-8 py-3 font-semibold text-center cursor-pointer select-none">
                                    @SharedLocalizer["Search"]
                                </button>
                            </div>
                        </div>
                    </form>
                </div>

            </div>
        </div>
        
    </div>
    <button id="success" hidden></button>
    <main class="app-wrapper" style="margin-bottom: 0;" dir="@(CultureInfo.CurrentCulture.Name.StartsWith("ar")?"rtl":"ltr")">
        @RenderBody()
    </main>
    <div id="wrap" dir="@(CultureInfo.CurrentCulture.Name.StartsWith("ar")?"rtl":"ltr")">
        @await Component.InvokeAsync("FooterSettings")
    </div>


    <script src="~/js/vue.js"></script>


    <script src="~/Admin/assets/js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="~/Admin/assets/js/vendor/jquery-3.3.1.min.js"></script>
    <script src="~/Admin/assets/js/vendor/popper.min.js"></script>

    <script src="~/devextreme/js/dx.all.js"></script>
    <script src="~/devextreme/js/localization/dx.messages.ru.js"></script>
    <script src="~/devextreme/js/dx.aspnet.data.js"></script>

    <script src="~/client/js/vendor/swiper.min.js"></script>
    <script src="~/client/js/slider.js" defer></script>

    <script src="~/js/x-notify.js"></script>
    <script src="~/client/js/header.js" defer></script>
    <script src="~/client/js/Video.js" defer></script>

<script>
        function myFunction() {
            var x = document.getElementById("myTopnav");
            if (x.className === "topnav") {
                x.className += " responsive";
            } else {
                x.className = "topnav";
            }
        }

        function toggleText() {
            var x = document.getElementById("Myid");
            if (x.style.display === "none") {
                x.style.display = "block";
            } else {
                x.style.display = "none";
            }
        }
        function OpenDict() {
            var x = document.getElementById("Dictionary");
            if (x.style.display === "none") {
                x.style.display = "block";
            } else {
                x.style.display = "none";
            }
        }

        function OpenSearch() {
            var x = document.getElementById("Search");
            if (x.style.display === "none") {
                x.style.display = "block";
            } else {
                x.style.display = "none";
            }
        }

        document.addEventListener("DOMContentLoaded", () => {
            const Notify = new XNotify();
            let success = document.getElementById("success");

                success.addEventListener("click", () => {
                    Notify.success({
                        title: "@SharedLocalizer["Thank you!"]",
                        description: "@SharedLocalizer["Your rating recorded."]",
                duration: 3000
            });
                });
        });

        function Copy() {

            navigator.clipboard.writeText("https://arinlang.com");

            /* Alert the copied text */
            alert("Copied the text: https://arinlang.com");
        }
         function CopyLink() {

            navigator.clipboard.writeText(" @Context.Request.Host@Context.Request.Path");

            /* Alert the copied text */
            alert("Link copied:  @Context.Request.Host@Context.Request.Path");
        }
</script>

    @RenderSection("Scripts", required: false)

</body>

</html>
